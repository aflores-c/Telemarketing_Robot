<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="telemarketingv5" args="left_motor_position_controller right_motor_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/telemarketingv5/joint_states"/>
</node>

</launch>